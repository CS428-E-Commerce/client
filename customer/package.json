{
  "name": "vinglish",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@emotion/styled": "^11.10.8",
    "@hookform/resolvers": "^2.8.8",
    "@mui/material": "^5.10.13",
    "@reduxjs/toolkit": "^1.9.3",
    "axios": "^0.21.1",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^5.2.3",
    "clsx": "^1.2.1",
    "connected-react-router": "^6.9.3",
    "dayjs": "^1.11.7",
    "eslint": "^7.29.0",
    "immer": "^9.0.21",
    "jquery": "^3.6.4",
    "lodash": "^4.17.21",
    "node-sass": "^6.0.1",
    "popper": "^1.0.1",
    "prettier": "^2.3.2",
    "prop-types": "^15.7.2",
    "qs": "^6.11.1",
    "react": "^17.0.2",
    "react-bootstrap": "^2.4.0",
    "react-bootstrap-editable": "^0.8.2",
    "react-bootstrap-sweetalert": "^5.2.0",
    "react-bootstrap-table-next": "^4.0.3",
    "react-bootstrap-table2-editor": "^1.4.0",
    "react-bootstrap-table2-paginator": "^2.1.2",
    "react-bootstrap-table2-toolkit": "^2.1.3",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.33.1",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^5.0.1",
    "react-select": "^4.3.1",
    "react-toastify": "^8.1.0",
    "redux": "^4.1.1",
    "redux-form": "^8.3.7",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "uuid": "^9.0.0",
    "yarn": "^1.22.19",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write \"**/*.{js,jsx,json,md,html,css,less,scss}\"",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "es6": true
    },
    "globals": {
      "$": true
    },
    "settings": {
      "react": {
        "version": "^17.0.1"
      }
    },
    "rules": {
      "use": "raw-loader",
      "test": "/\\.js|\\.jsx$/",
      "semi": [
        "error",
        "never"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "react-error-overlay": "6.0.9",
    "@types/react": "^17.0.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-react": "^7.20.6",
    "postcss": "^8.4.24",
    "prettier": "^2.1.1",
    "react-error-overlay": "^6.0.9",
    "tailwindcss": "^3.3.2"
  }
}
